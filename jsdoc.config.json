{
    "plugins": ["plugins/markdown"],
    "recurseDepth": 10,
    "source": {
        "include": "snake",
        "exclude": [".vscode", ".github", "coverage", "node_modules", "test"],
        "includePattern": ".+\\.js$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
        "encoding": "utf8",
        "private": false,
        "recurse": true,
        "theme_opts": {
            "theme": "dark"
        }
    }
}